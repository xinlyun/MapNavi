<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/outside_ll"
    >
    <FrameLayout
        android:background="@drawable/shadowss"

        android:paddingLeft="9px"
        android:paddingRight="9px"
        android:paddingTop="4px"
        android:paddingBottom="14px"
        android:layout_marginLeft="90px"
        android:layout_marginTop="8px"
        android:layout_width="900px"
        android:layout_height="1016px"

        >
        <LinearLayout
            android:id="@+id/inside_ll"
            android:orientation="vertical" android:layout_width="match_parent"
            android:background="@drawable/white_backgrouad"
            android:padding="1px"
            android:layout_height="match_parent">


            <LinearLayout
                android:layout_marginTop="12px"
                android:id="@+id/prepare_edit_bar"
                android:orientation="horizontal"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="48px">
                <ImageView
                    android:id="@+id/btn_return"
                    android:src="@drawable/icon_return"
                    android:layout_width="72px"
                    android:layout_height="match_parent" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:background="@color/f0f0f0"
                    android:layout_width="796px"

                    android:layout_height="match_parent">
                    <ImageView
                        android:src="@drawable/icon_search_02"
                        android:layout_width="48px"
                        android:layout_height="match_parent" />
                    <EditText
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:background="@null"
                        android:id="@+id/prepare_edittext"
                        android:textSize="24px"

                        android:textColor="@color/first_text_color"
                        android:hint="搜索目的地"

                        android:textColorHint="@color/a3a3a3"
                        android:layout_width="660px"
                        android:layout_height="match_parent" />
                    <Button
                        android:id="@+id/btn_search"
                        android:textColor="@color/white"
                        android:textSize="24px"
                        android:text="@string/search"
                        android:background="@color/text_blue"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                </LinearLayout>
                <Button
                    android:id="@+id/pre_beginnavi"
                    android:text="搜索"
                    android:textSize="50px"
                    android:textColor="@color/Grey"
                    android:background="@drawable/prepare_seach_btn_false"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="37px"
                    android:layout_marginRight="37px"
                    android:layout_width="194px"
                    android:layout_height="116px" />

            </LinearLayout>
            <!--<ImageView-->
                <!--android:background="@color/gray_btn_bg_pressed_color"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="1px" />-->
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:gravity="center_vertical"
                android:layout_height="99px">
                <LinearLayout
                    android:background="@drawable/text_trans_bg"
                    android:id="@+id/btn_go_collect"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:src="@drawable/icon_collect_1"
                        android:layout_width="48px"

                        android:layout_height="48px" />
                    <TextView
                        android:layout_marginLeft="12px"
                        android:gravity="center_vertical"
                        android:textStyle="bold"
                        android:textSize="26px"
                        android:textColor="@color/first_text_color"

                        android:layout_width="wrap_content"
                        android:text="@string/collor_poi"
                        android:layout_height="match_parent" />
                </LinearLayout>
                <ImageView
                    android:background="@color/gray_btn_bg_pressed_color"
                    android:layout_width="1px"
                    android:layout_height="40px" />
                <LinearLayout
                    android:id="@+id/btn_start_power"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:src="@drawable/icon_power"
                        android:layout_width="48px"

                        android:layout_height="48px" />
                    <TextView
                        android:layout_marginLeft="12px"
                        android:gravity="center_vertical"
                        android:textSize="26px"
                        android:text="@string/power_dong"
                        android:textColor="@color/first_text_color"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent" />
                </LinearLayout>

            </LinearLayout>
            <ImageView
                android:background="@color/gray_btn_bg_pressed_color"
                android:layout_width="match_parent"
                android:layout_height="1px" />
            <com.xiaopeng.xmapnavi.view.appwidget.selfview.DelSlideListView
                android:id="@+id/lv_show_tip_his"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </com.xiaopeng.xmapnavi.view.appwidget.selfview.DelSlideListView>


        </LinearLayout>
    </FrameLayout>
</FrameLayout>

