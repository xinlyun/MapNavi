<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="880px"
    android:minHeight="109px"
    android:layout_height="109px"
    android:orientation="vertical"
    >
    <LinearLayout

        android:orientation="horizontal" android:layout_width="880px"
        android:id="@+id/back_ll"
        android:minHeight="108px"
        android:layout_height="108px">

        <LinearLayout
            android:layout_marginLeft="40px"
            android:orientation="vertical"
            android:gravity="center_vertical"
            android:layout_width="752px"
            android:minHeight="108px"
            android:layout_height="108px">
            <TextView
                android:singleLine="true"
                android:layout_gravity="center_vertical"
                android:textColor="@color/first_text_color"
                android:id="@+id/pre_list_name"

                android:textSize="28px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_gravity="center_vertical"
                android:singleLine="true"
                android:textColor="@color/a3a3a3"
                android:textSize="24px"
                android:id="@+id/pre_list_posi"
                android:layout_marginTop="10px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <ImageView
            android:id="@+id/touch_to_navi"
            android:src="@drawable/icon_collect_1"
            android:layout_width="88px"
            android:layout_height="match_parent" />
    </LinearLayout>
    <ImageView
        android:background="@color/gray_btn_bg_pressed_color"
        android:layout_width="match_parent"
        android:layout_height="1px" />
</LinearLayout>
