<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_out_side"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--android:layout_marginLeft="240px"-->
    <!--android:layout_marginTop="800px"-->
    <FrameLayout

        android:id="@+id/second_framelayout"
        android:background="@drawable/shadowss"
        android:paddingLeft="9px"
        android:paddingRight="9px"
        android:paddingBottom="14px"
        android:paddingTop="4px"
        android:layout_marginTop="8px"
        android:layout_marginLeft="92px"
        android:layout_width="896px"
        android:layout_height="774px">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:padding="1px"
            android:background="@drawable/white_backgrouad"
            android:layout_height="match_parent">
            <RelativeLayout

                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"

                android:background="@color/white"

                android:layout_height="72px">
                <ImageView
                    android:id="@+id/btn_return"
                    android:scaleType="center"
                    android:src="@drawable/icon_return"
                    android:layout_width="88px"
                    android:layout_height="72px" />
                <TextView

                    android:textSize="30px"
                    android:textColor="@color/first_text_color"
                    android:gravity="center"
                    android:text="@string/setting"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/office_map"
                    android:gravity="center"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="24px"
                    android:textColor="@color/text_blue"
                    android:textSize="24px"
                    android:text="@string/offine_map"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
            </RelativeLayout>
            <ImageView

                android:background="@color/gray_btn_bg_pressed_color"
                android:layout_width="match_parent"
                android:layout_height="1px" />
            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="92px">
                <ImageView
                    android:background="@color/gray_btn_bg_pressed_color"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1px" />
                <LinearLayout
                    android:layout_marginLeft="102px"
                    android:orientation="vertical"
                    android:layout_width="238px"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/tx_first_view"
                        android:textColor="@color/text_blue"
                        android:gravity="center"
                        android:textSize="30px"
                        android:paddingTop="49px"
                        android:text="@string/map_setting"
                        android:layout_width="match_parent"
                        android:layout_height="84px" />
                    <ImageView
                        android:layout_marginLeft="40px"
                        android:layout_marginRight="40px"
                        android:id="@+id/iv_first_view"
                        android:layout_marginTop="3px"
                        android:background="@color/text_blue"
                        android:layout_width="match_parent"
                        android:layout_height="4px" />
                </LinearLayout>

                <LinearLayout
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="102px"
                    android:orientation="vertical"
                    android:layout_width="238px"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/tx_second_view"
                        android:textColor="@color/first_text_color"
                        android:gravity="center"
                        android:textSize="30px"
                        android:paddingTop="48px"
                        android:text="@string/collect_like"
                        android:layout_width="match_parent"
                        android:layout_height="84px" />
                    <ImageView
                        android:layout_marginLeft="40px"
                        android:layout_marginRight="40px"
                        android:id="@+id/iv_second_view"
                        android:layout_marginTop="3px"
                        android:background="@color/transparent"
                        android:layout_width="match_parent"
                        android:layout_height="4px" />
                </LinearLayout>
            </RelativeLayout>

            <android.support.v4.view.ViewPager
                android:id="@+id/view_pager"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </android.support.v4.view.ViewPager>
        </LinearLayout>

    </FrameLayout>
</RelativeLayout>